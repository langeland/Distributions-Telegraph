{
    "name": "langeland/distribution-telefraph",
    "description": "Platform for building and managing digital telegrams",
    "license": "MIT",
    "support": {
        "email": "hello@neos.io",
        "slack": "http://slack.neos.io/",
        "forum": "https://discuss.neos.io/",
        "wiki": "https://discuss.neos.io/c/the-neos-project/project-documentation",
        "issues": "https://github.com/neos/flow-development-collection/issues",
        "docs": "https://flowframework.readthedocs.io/",
        "source": "https://github.com/neos/flow-base-distribution"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true,
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-tokenizer": "*",
        "neos/flow": "~8.3.0",
        "jakoch/phantomjs-installer": "^3.0.0",
        "langeland/telegraphcore": "@dev",
        "langeland/telegraphuserinterface": "@dev",
        "langeland/telegraphapi": "@dev"
    },
    "require-dev": {
        "neos/kickstarter": "~8.3.0"
    },
    "replace": {
        "typo3/flow-base-distribution": "self.version"
    },
    "repositories": {
        "distributionPackages": {
            "type": "path",
            "url": "./DistributionPackages/*"
        }
    },
    "suggest": {
        "ext-pdo_sqlite": "For running functional tests out-of-the-box this is required",
        "neos/fluid-adaptor": "For rendering templates with TYPO3.Fluid"
    },
    "scripts": {
        "post-install-cmd": [
            "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
